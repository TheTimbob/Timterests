package web

import (
    "timterests/internal/models"
)

templ AboutForm(about models.About) {
	@Base() {
		<div id="about-container">
            <h1 class="category-title">{ about.Title }</h1>
            <h2 class="category-subtitle">{ about.Subtitle }</h2>
	        @templ.Raw(about.Body)
        </div>
	}
}
